    <rdf:type rdf:resource="http://schema.org/Organization" />
    <owl:sameAs rdf:resource="http://www.wikidata.org/entity/Q1351501" />
    <rdfs:label xml:lang="en">Gilberto Rodríguez Orejuela</rdfs:label>
    <rdfs:comment xml:lang="en">Gilberto José Rodríguez Orejuela (born on January 30, 1939, in Mariquita, Tolima) is a Colombian druglord, formerly one of the leaders of the Cali Cartel, based in the city of Cali.</rdfs:comment>
    <schema:name xml:lang="en">Gilberto Rodríguez Orejuela</schema:name>
    <foaf:isPrimaryTopicOf rdf:resource="http://en.wikipedia.org/wiki/Gilberto_Rodríguez_Orejuela" />
