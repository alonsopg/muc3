    <owl:sameAs rdf:resource="http://www.wikidata.org/entity/Q326239" />
    <rdfs:label xml:lang="en">Barry Seal</rdfs:label>
    <rdfs:comment xml:lang="en">Adler Berriman Seal (July 16, 1939 – February 19, 1986), better known as Barry Seal, was a United States drug smuggler and aircraft pilot and dealer who flew flights for the Medellín Cartel.</rdfs:comment>
    <schema:name xml:lang="en">Barry Seal</schema:name>
    <foaf:isPrimaryTopicOf rdf:resource="http://en.wikipedia.org/wiki/Barry_Seal" />
